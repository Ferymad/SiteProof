{
  "version": 2,
  "builds": [
    {
      "src": "apps/web/next.config.js",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/web/$1"
    }
  ],
  "installCommand": "npm install && npm run build:web",
  "buildCommand": "cd apps/web && npm run build",
  "outputDirectory": "apps/web/.next",
  "framework": "nextjs",
  "functions": {
    "apps/web/pages/api/**/*.ts": {
      "runtime": "nodejs18.x"
    }
  }
}